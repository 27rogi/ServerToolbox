{
  "schemaVersion": 1,
  "id": "server-toolbox",
  "version": "${version}",
  "name": "Server Toolbox",
  "description": "A server-side mod with complect of customizable commands and features to make your server neat. Inspired by Essentials, Essential Commands, plugins and etc.",
  "authors": [
    "27rogi"
  ],
  "contact": {
    "homepage": "https://rogi.su/",
    "sources": "https://github.com/rogi27/ServerToolbox"
  },
  "license": "GNU GPL 3.0",
  "icon": "assets/server-toolbox/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "space.ryzhenkov.servertoolbox.ServerToolbox",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "server-toolbox.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.21",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.8.22"
  }
}